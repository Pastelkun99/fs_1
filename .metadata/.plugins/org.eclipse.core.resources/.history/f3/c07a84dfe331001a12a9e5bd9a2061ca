package com.fusion1.controller;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		 
		Scanner scan = new Scanner(System.in);
		
		int testCase = scan.nextInt();
		
		for(int i = 0; i<testCase; i++) {
			
			int student = scan.nextInt();
			int testCaseSum = 0;
			int[] scoreArr = new int[student];
			
			for(int k = 0; k<student; k++) {
				scoreArr[k] = scan.nextInt();
				testCaseSum += scoreArr[k];
			}
			
			double testCaseAvg = testCaseSum / student;
			double count = 0;
			
			for(int j = 0; j < student; j++) {
				if(scoreArr[j] > testCaseAvg) {
					++count;
				}
			}
			
			double persent = (count / student) * 100;
			System.out.println(Math.round(persent*1000)/1000 + "%");
			
		}
		
		
		
	}
}