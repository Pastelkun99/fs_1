<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
					"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="logMapper">

	<select id="getLogList" resultType="com.fusion1.dao.LogVO">
		SELECT * FROM LOGTBL
	</select>
	
	<insert id="logWrite" parameterType="com.fusion1.dao.LogVO">
		INSERT INTO 
					LOGTBL VALUES (0, 
								   #{log_userid}, 
								   now(),
								   #{log_userip}, 
								   #{log_userbrowser}, 
								   #{log_bversion}, 
								   #{log_osversion}, 
								   #{log_userreferrer}
								   )
	</insert>
	
	<select id="getLogNameList" resultType="com.fusion1.dao.LogVO">
		SELECT log_userid, COUNT(*) AS CNT FROM LOGTBL GROUP BY log_userid ORDER BY cnt DESC;
	</select>
	
	<select id="getLogOSList" resultType="com.fusion1.dao.LogVO">
		SELECT log_osversion, COUNT(*) AS CNT FROM LOGTBL GROUP BY log_osversion ORDER BY cnt DESC;
	</select>
	
	<select id="getLogBrowserList" resultType="com.fusion1.dao.LogVO">
		SELECT log_userbrowser, COUNT(*) AS CNT FROM LOGTBL GROUP BY log_userbrowser ORDER BY cnt DESC;
	</select>
	
	<delete id="logDelete" parameterType="com.fusion1.dao.LogVO">
		DELETE FROM LOGTBL WHERE log_no = #{log_no}
	</delete>
	
	
</mapper>