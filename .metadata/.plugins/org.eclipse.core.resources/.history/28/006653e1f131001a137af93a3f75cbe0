package com.fusion1.controller;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int digit = scan.nextInt();
		int sum = 0;
		
		String inputNumber = scan.next();
		if(inputNumber.length() != digit) {
			System.exit(0);
		}
		
		char[] scatteredArr = inputNumber.toCharArray();
		for(int i = 0; i<scatteredArr.length; i++) {
			String slice = Character.toString(scatteredArr[i]);
			int sliceInt = Integer.parseInt(slice);
			sum += sliceInt;
		}
		
		System.out.println(sum);
		
	}
}