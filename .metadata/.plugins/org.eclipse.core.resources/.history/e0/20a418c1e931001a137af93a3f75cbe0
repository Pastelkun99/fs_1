package com.fusion1.controller;

public class Main {

	public static void main(String[] args) {
		
		boolean[] isNotSelfNumber = new boolean[10001];
		
		for(int i = 1; i<= 10000; i++) {
			int dn = evaluateNotSelfNumber(i);
			if (dn <= 10000) {
				isNotSelfNumber[dn] = true;
			}
		}
		
		for(int i = 1; i< isNotSelfNumber.length; i++) {
			if (!isNotSelfNumber[i]) {
				System.out.println(i);
			}
		}
		
	}
	
	private static int evaluateNotSelfNumber(int number) {
		
		int notSelfNumber = number;
		
		while(number > 0) {
			notSelfNumber += number % 10;
			number /= 10;
		}
		
		return notSelfNumber;
	}
}